		<trigger name="TR_CT_CT_MH_AFT_INS_UPD" id="Trigger_37066452" isSystem="false" >
			<string><![CDATA[CREATE OR REPLACE TRIGGER "QLBH"."TR_CT_CT_MH_AFT_INS_UPD" 
 AFTER INSERT  OR UPDATE OR DELETE
 ON CHI_TIET_CHUNG_TU_MUA_HANG
BEGIN
    UPDATE CHUNG_TU_MUA_HANG CT
    SET CT.CONG_TIEN_HANG  =
    (
     SELECT SUM(CTCT.SO_LUONG*CTCT.DON_GIA)
     FROM CHI_TIET_CHUNG_TU_MUA_HANG CTCT
     WHERE CTCT.CHUNG_TU_MUA_HANG_ID =CT.ID
     );
    UPDATE CHUNG_TU_MUA_HANG CT
    SET CT.TIEN_SAU_CHIET_KHAU = CT.CONG_TIEN_HANG  -CT.CHIET_KHAU ;
 END;
]]></string>
		</trigger>
		<trigger name="TR_CT_CT_MH_AF_INS_U" id="Trigger_195984a2" isSystem="false" >
			<string><![CDATA[CREATE OR REPLACE TRIGGER "QLBH"."TR_CT_CT_MH_AF_INS_U" 
 BEFORE INSERT  OR UPDATE
 ON CHI_TIET_CHUNG_TU_MUA_HANG
 REFERENCING NEW AS NEW
 FOR EACH ROW
BEGIN
  SELECT CT.THUE_VAT ,(CT.THUE_VAT/100)*:NEW.DON_GIA +:NEW.DON_GIA  INTO :NEW.THUE_VAT ,:NEW.GIA_SAU_VAT
  FROM CHUNG_TU_MUA_HANG CT
  WHERE CT.ID = :new.chung_tu_mua_hang_id ;
 END;
]]></string>
		</trigger>
		<trigger name="TR_CT_CT_MH_BEF_INS_UPD" id="Trigger_1f87a6fc" isSystem="false" >
			<string><![CDATA[CREATE OR REPLACE TRIGGER "QLBH"."TR_CT_CT_MH_BEF_INS_UPD" 
 BEFORE INSERT  OR UPDATE
 ON CHI_TIET_CHUNG_TU_MUA_HANG
 REFERENCING NEW AS NEW
 FOR EACH ROW
DECLARE
 v_Hang_Hoa_Don_Vi_Tinh_ID NUMBER:=-1;
 v_Hang_hoa_Ten_Don_Vi_Tinh DON_VI_TINH.TEN_DON_VI_TINH%Type:='';
 v_Ten_Don_Vi_Tinh_Nhap DON_VI_TINH.TEN_DON_VI_TINH%TYPE:='';
BEGIN
  SELECT  H.DON_VI_TINH_ID  INTO v_Hang_Hoa_Don_Vi_Tinh_ID FROM HANG_HOA H WHERE H.ID = :new.hang_hoa_id ;
  /*
  If v_Hang_Hoa_Don_Vi_Tinh_ID <> :new.Don_Vi_Tinh_Id THEN
     SELECT DT.TEN_DON_VI_TINH into v_Hang_hoa_Ten_Don_Vi_Tinh  FROM DON_VI_TINH DT WHERE DT.ID =v_Hang_Hoa_Don_Vi_Tinh_ID ;
     SELECT DT.TEN_DON_VI_TINH into v_Ten_Don_Vi_Tinh_Nhap  FROM DON_VI_TINH DT WHERE DT.ID =:new.Don_Vi_Tinh_Id ;
     raise_application_error(-20001 ,' Don Vi Tinh cua Hang Hoa la '||v_Hang_hoa_Ten_Don_Vi_Tinh||' khac voi  Don Vi Tinh dang nhap '||v_Ten_Don_Vi_Tinh_Nhap) ;
  end if ;
  */SELECT CT.THUE_VAT ,(CT.THUE_VAT/100)*:NEW.DON_GIA +:NEW.DON_GIA  INTO :NEW.THUE_VAT ,:NEW.GIA_SAU_VAT
  FROM CHUNG_TU_MUA_HANG CT
  WHERE CT.ID = :new.chung_tu_mua_hang_id ;
 SELECT H.DON_VI_QUI_DOI_ID INTO :NEW.DON_VI_TINH_QUI_DOI_ID
 FROM HANG_HOA H
 WHERE H.ID  =:new.Hang_Hoa_Id  ;
 SELECT H.SO_LUONG_QUI_DOI INTO :NEW.QUI_DOI
 FROM HANG_HOA H
 WHERE H.ID  =:new.Hang_Hoa_Id  ;
 --Neu so luong va qui doi co' thi tinh' soluong sau khi qui doi
 IF :new.Qui_Doi IS NOT NULL and :new.So_Luong IS NOT NULL THEN
     :new.So_Luong_Sau_Qui_Doi :=:new.So_Luong/:new.Qui_Doi ;
 END IF ;
 END;
]]></string>
		</trigger>
		<trigger name="TR_CT_CT_MH_BEF_UP" id="Trigger_fe57cea" isSystem="false" >
			<string><![CDATA[CREATE OR REPLACE TRIGGER "QLBH"."TR_CT_CT_MH_BEF_UP" 
